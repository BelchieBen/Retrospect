        {
          "expr": "sum(count_over_time({service=\"retrospect-server\"} |~ \"payload received\" [1h]))",
          "legendFormat": "Notifications/Events",
          "refId": "F",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
